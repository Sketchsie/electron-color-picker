{
  "name": "electron-color-picker",
  "version": "0.1.3-dev.0",
  "description": "Pick color from Desktop, suitable for use with Electron.",
  "author": "mockingbot",
  "license": "MIT",
  "keywords": [
    "Electron",
    "ColorPicker"
  ],
  "repository": "github:mockingbot/electron-color-picker",
  "main": "library/index.js",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node ./script quiet",
    "script-pack": "babel-node ./script quiet pack",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "script-generate-spec": "babel-node ./script/generateSpec",
    "// build =======================": "",
    "build-library": "babel ./source --out-dir ./output-gitignore/library --copy-files",
    "build-library-dev": "cross-env BABEL_ENV=dev npm run build-library -- --watch",
    "// =============================": "",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "engines": {
    "node": ">=8.2.1",
    "npm": ">=6"
  },
  "peerDependencies": {
    "electron": ">=1.8.1"
  },
  "devDependencies": {
    "dr-dev": "0.0.6-dev.3",
    "dr-dev-babel": "0.0.6-dev.3"
  }
}
